# mount point       fstype    device                                                                flags

### LOGICAL ###
/system             ext4      /dev/block/platform/bootdevice/by-name/system				flags=display="System";backup=1;logical;avb=vbmeta;
/system_root        emmc      /dev/block/platform/bootdevice/by-name/system				flags=display="System image";backup=1;flashimg=1;logical;avb=vbmeta;
/vendor             ext4      /dev/block/platform/bootdevice/by-name/vendor				flags=display="Vendor";backup=1;logical;avb,
/vendor_image       emmc      /dev/block/platform/bootdevice/by-name/vendor				flags=display="Vendor image";backup=1;flashimg=1;logical;avb,
/product            ext4      /dev/block/platform/bootdevice/by-name/product				flags=display="Product";backup=1;logical;avb,
/product_image      emmc      /dev/block/platform/bootdevice/by-name/product				flags=display="Product image";backup=1;flashimg=1;logical;avb,
/odm                ext4      /dev/block/platform/bootdevice/by-name/odm				flags=display="Odm";backup=1;logical;avb,
/odm_image          emmc      /dev/block/platform/bootdevice/by-name/odm				flags=display="Odm image";backup=1;flashimg=1;logical;avb,
/boot               emmc      /dev/block/platform/bootdevice/by-name/boot				flags=display="Boot";backup=1;flashimg=1;
/recovery           emmc      /dev/block/platform/bootdevice/by-name/recovery				flags=display="Recovery";backup=1;flashimg=1;

/data               f2fs      /dev/block/platform/bootdevice/by-name/userdata				flags=fileencryption=ice:aes-256-cts;display="Userdata";backup=1;wipeingui
/cache              ext4      /dev/block/platform/bootdevice/by-name/cache				flags=display="Cache";backup=1;wipeingui
/efs                ext4      /dev/block/platform/bootdevice/by-name/efs				flags=display="Efs";backup=1
/carrier            ext4      /dev/block/platform/bootdevice/by-name/carrier				flags=display="Carrier";
/preload            ext4      /dev/block/platform/bootdevice/by-name/hidden				flags=display="Preload";
/misc               emmc      /dev/block/platform/bootdevice/by-name/misc				flags=display="Misc";backup=1;flashimg=1;
/metadata           ext4      /dev/block/platform/bootdevice/by-name/md_udc				flags=display="Metadata";backup=1;

/external_sd	    vfat      /dev/block/mmcblk1p1	/dev/block/mmcblk1				flags=display="SD Card";storage;wipeingui;removable
/usb-otg	    vfat      /dev/block/sda1	/dev/block/sda						flags=display="USB-OTG";storage;wipeingui;removable

/md1dsp             emmc      /dev/block/platform/bootdevice/by-name/md1dsp			    flags=display="Dsp";
/dtbo               emmc      /dev/block/platform/bootdevice/by-name/dtbo                           flags=display="Dtbo";backup=1;flashimg=1;
/md1img             emmc      /dev/block/platform/bootdevice/by-name/md1img                         flags=display="Md1img";
/spmfw              emmc      /dev/block/platform/bootdevice/by-name/spmfw                          flags=display="Spmfw";
/scp1               emmc      /dev/block/platform/bootdevice/by-name/scp1                           flags=display="Scp1";
/sspm_1             emmc      /dev/block/platform/bootdevice/by-name/sspm_1                         flags=display="Sspm_1";
/keydata            ext4      /dev/block/platform/bootdevice/by-name/keydata                        flags=display="Keydata";
/keyrefuge          ext4      /dev/block/platform/bootdevice/by-name/keyrefuge                      flags=display="Keyrefuge";
/gz1                emmc      /dev/block/platform/bootdevice/by-name/gz1                            flags=display="Gz1";
/optics             ext4      /dev/block/platform/bootdevice/by-name/optics                         flags=display="Optics";
/prism              ext4      /dev/block/platform/bootdevice/by-name/prism                          flags=display="Prism";
/vbmeta_samsung     emmc      /dev/block/platform/bootdevice/by-name/vbmeta_samsung                 flags=display="Vbmeta_samsung";
/vbmeta_vendor      emmc      /dev/block/platform/bootdevice/by-name/vbmeta_vendor                  flags=display="Vbmeta_vendor";
