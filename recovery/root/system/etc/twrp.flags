# mount point	fstype		device			device2				flags
# Logical partitions
system		/system	ext4							wait,logical,first_stage_mount
vendor		/vendor	ext4							wait,logical,first_stage_mount
product	/product	ext4							wait,logical,first_stage_mount
odm		/odm		ext4							wait,logical,first_stage_mount




# EMMC Points
/boot               emmc    /dev/block/platform/bootdevice/by-name/boot         flags=display="Kernel Image";backup=1;flashimg=1
/recovery           emmc    /dev/block/platform/bootdevice/by-name/recovery     flags=display="Recovery Image";backup=1;flashimg=1
/dtbo               emmc    /dev/block/platform/bootdevice/by-name/dtbo         flags=display="DTBO Image";backup=1;flashimg=1
/optics_image       emmc    /dev/block/platform/bootdevice/by-name/optics       flags=display="Optics Image";backup=1;flashimg=1
/prism_image        emmc    /dev/block/platform/bootdevice/by-name/prism        flags=display="Prism Image";backup=1;flashimg=1
/vbmeta             emmc    /dev/block/platform/bootdevice/by-name/vbmeta                flags=backup=1;display="Vbmeta Image";flashimg=1
/vbmeta_system      emmc    /dev/block/platform/bootdevice/by-name/vbmeta_system         flags=backup=1;display="Vbmeta_system Image";flashimg=1
/vbmeta_samsung     emmc    /dev/block/platform/bootdevice/by-name/vbmeta_samsung        flags=backup=1;display="Vbmeta_Samsung Image";flashimg=1

/system_image       emmc      system                    flags=display="System image";backup=1;flashimg=1
/vendor_image       emmc      vendor                             flags=display="Vendor image";backup=1;flashimg=1
/product_image      emmc      product                    flags=display="Product image";backup=1;flashimg=1
/odm_image          emmc      odm                                         flags=display="Odm image";backup=1;flashimg=1

# Cache and ETC Points
/cache              ext4    /dev/block/platform/bootdevice/by-name/cache         flags=display="Cache";backup=1;wipeingui
/efs                emmc         /dev/block/bootdevice/by-name/sec_efs               flags=display="EFS";backup=1
/efs		ext4		/dev/block/platform/bootdevice/by-name/sec_efs		flags=backup=1;display="EFS"
/modem		emmc		/dev/block/platform/bootdevice/by-name/apnhlos		flags=backup=1;display="Modem"
/mdm		emmc		/dev/block/platform/bootdevice/by-name/modem
/dsp		emmc		/dev/block/platform/bootdevice/by-name/dsp
/dtbo		emmc		/dev/block/platform/bootdevice/by-name/dtbo
/md1img		emmc		/dev/block/platform/bootdevice/by-name/md1img
/spmfw		emmc		/dev/block/platform/bootdevice/by-name/spmfw
/scp1		emmc		/dev/block/platform/bootdevice/by-name/scp1
/sspm_1		emmc		/dev/block/platform/bootdevice/by-name/sspm_1	
/data               f2fs    /dev/block/platform/bootdevice/by-name/userdata      flags=display="Userdata";backup=1;wipeingui;length=-16384
/misc		emmc		/dev/block/platform/bootdevice/by-name/misc		flags=backup=1;display="Misc"

# Optics, SEC and Prism Points
/prism              ext4    /dev/block/platform/bootdevice/by-name/prism         flags=display="Prism";backup=1
/optics             ext4    /dev/block/platform/bootdevice/by-name/optics        flags=display="Optics";backup=1

# Metadata Point
/metadata           ext4    /dev/block/platform/bootdevice/by-name/md_udc      flags=display="Metadata";backup=1;wipeingui

# Removable storage
/external_sd	vfat		/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="External_sd";storage;wipeingui;removable
/usb-otg	auto		/dev/block/sda1	/dev/block/sda			flags=display="USB-OTG";storage;wipeingui;removable
